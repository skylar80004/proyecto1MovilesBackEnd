<% if signed_in? %>
<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% end %>
